        }
      ]
    },
    {
      "name": "ruler.rule-config.init-type",
      "values": [
        {
          "value": "db",
          "description": "从数据库初始化规则信息."
        },
        {
          "value": "annotation",
          "description": "从配置类注解初始化规则信息."
        }
      ]
    },
    {
      "name": "ruler.rules-engine-config.type",
      "values": [
        {
          "value": "simple",
          "description": "简单的规则引擎，仅关心约束对象是否满足规则约束."
        },
        {
          "value": "incomplete",
          "description": "不完全详细的规则引擎，能够返回校验的结果报告，并在违规的时候会结束校验."
        },
        {
          "value": "complete",
          "description": "完全详细的规则引擎，能够返回校验的结果报告，会执行规则集合中的所有规则."
        }
      ]
    }
